<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity4"
    android:gravity="center"
    android:orientation="vertical">


    <TextView
        android:id="@+id/vers_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minHeight="48dp"
        android:onClick="onClickGoMain3"
        android:text="@string/vers4"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <Button
        android:id="@+id/next_button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/next4"
        app:layout_constraintEnd_toEndOf="@+id/vers_4"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="@+id/vers_4"
        app:layout_constraintTop_toBottomOf="@+id/vers_4" />

</androidx.constraintlayout.widget.ConstraintLayout>