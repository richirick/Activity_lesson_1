<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity2">


  <TextView
      android:id="@+id/vers_2"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:padding="24dp"
      android:textAlignment="center"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      android:text="@string/vers2" />


  <Button
      android:id="@+id/next_button2"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:onClick="onClickGoMain1"
      android:text="@string/next2"
      app:layout_constraintEnd_toEndOf="@+id/vers_2"
      app:layout_constraintHorizontal_bias="0.493"
      app:layout_constraintStart_toStartOf="@+id/vers_2"
      app:layout_constraintTop_toBottomOf="@+id/vers_2" />

</androidx.constraintlayout.widget.ConstraintLayout>