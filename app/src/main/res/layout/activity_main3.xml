<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity3" >


    <TextView
        android:id="@+id/vers_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/vers3"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.457" />

    <Button
        android:id="@+id/next_button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:onClick="onClickGoMain2"
        android:text="@string/next3"
        app:layout_constraintEnd_toEndOf="@+id/vers_3"
        app:layout_constraintStart_toStartOf="@+id/vers_3"
        app:layout_constraintTop_toBottomOf="@+id/vers_3" />

</androidx.constraintlayout.widget.ConstraintLayout>